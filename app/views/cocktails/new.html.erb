<div class="banner" style="background-image: linear-gradient(rgba(0,0,0,0.4),rgba(0,0,0,0.4)), url(http://www.ful-filled.com/wp-content/uploads/2017/04/IMG_6702-2.jpg);">
  <div class="container">
    <div class="row">
      <div class="col-sm-8">
        <h1>Add a Boba Card</h1>
        <%= simple_form_for @cocktail do |f| %>
          <%= f.input :name %>
          <%= f.input :photo %>
          <%= f.input :photo_cache, as: :hidden %>
          <%= f.submit %>
        <% end %>
        <div class="mt-3"><%= link_to "Back to List", cocktails_path, class: "btn btn-danger"%></div>
      </div>
    </div>
  </div>
</div>