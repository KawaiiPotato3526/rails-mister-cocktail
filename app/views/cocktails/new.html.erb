<div class="container">
  <div class="row">
    <div class="col-sm-8">
      <h1>Add A Cocktail Card</h1>
      <%= simple_form_for @cocktail do |f| %>
        <%= f.input :name %>
        <%= f.input :photo %>
        <%= f.input :photo_cache, as: :hidden %>
        <%= f.submit %>
      <% end %>
      <%= link_to "Back to List", cocktails_path, class: "btn btn-danger"%>
    </div>
  </div>
</div>